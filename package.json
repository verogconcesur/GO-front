{
    "name": "Concenet_Front",
    "version": "1.1.5",
    "jenga_FrontVersion": "1.3.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --hmr",
        "build": "ng build",
        "build_dev": "npm run build -- --configuration=dev",
        "build_qa": "npm run build -- --configuration=qa",
        "build_pre": "npm run build -- --configuration=pre",
        "build_stg": "npm run build -- --configuration=stg",
        "build_go_des": "npm run build -- --configuration=go_des",
        "build_production": "npm run build -- --configuration=production",
        "build_production_la_baronia": "npm run build -- --configuration=production_la_baronia",
        "build_production_servidauto": "npm run build -- --configuration=production_servidauto",
        "test": "ng test --code-coverage",
        "test:ci": "ng test --watch=false --code-coverage",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "sonar": "npm run test:ci && sonar-scanner",
        "extract:translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/es.json ./src/assets/i18n/en.json --sort --format namespaced-json",
        "release:first": "standard-version --no-verify --first-release",
        "release:patch": "standard-version --no-verify --release-as patch",
        "release:minor": "standard-version --no-verify --release-as minor",
        "release:major": "standard-version --no-verify --release-as major",
        "prebundle:report": "npm run build_production",
        "bundle:report": "source-map-explorer dist/Concenet_Front/**/*.js"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run test:ci",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && npm run lint"
        }
    },
    "resolutions": {
        "webpack": "^5.0.0"
    },
    "private": true,
    "dependencies": {
        "@angular-material-components/datetime-picker": "^8.0.0",
        "@angular-material-components/moment-adapter": "^8.0.0",
        "@angular/animations": "^14.2.3",
        "@angular/cdk": "^14.2.3",
        "@angular/common": "^14.2.3",
        "@angular/compiler": "^14.2.3",
        "@angular/core": "^14.2.3",
        "@angular/flex-layout": "^14.0.0-beta.40",
        "@angular/forms": "^14.2.3",
        "@angular/material": "^14.2.2",
        "@angular/platform-browser": "^14.2.3",
        "@angular/platform-browser-dynamic": "^14.2.3",
        "@angular/router": "^14.2.3",
        "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
        "@messageformat/core": "^3.0.1",
        "@ngneat/until-destroy": "^9.0.2",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
        "@stomp/rx-stomp": "1.2.0",
        "file-saver": "^2.0.5",
        "hammerjs": "^2.0.8",
        "husky": "^4.3.0",
        "jquery": "^3.6.0",
        "jqueryui": "^1.11.1",
        "lodash.clonedeep": "^4.5.0",
        "messageformat": "^2.3.0",
        "miragejs": "^0.1.45",
        "moment": "^2.29.4",
        "net": "^1.0.2",
        "ngx-extended-pdf-viewer": "^15.0.12",
        "ngx-logger": "^4.1.9",
        "ngx-summernote": "^0.8.8",
        "ngx-translate-messageformat-compiler": "^5.1.0",
        "p5": "^1.5.0",
        "rxjs": "^7.0.0",
        "signature_pad": "^4.1.5",
        "stompjs": "^2.3.3",
        "summernote": "^0.8.20",
        "tslib": "^2.0.0",
        "ws": "^8.12.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.2.3",
        "@angular-eslint/builder": "^14.1.2",
        "@angular-eslint/eslint-plugin": "^14.1.2",
        "@angular-eslint/eslint-plugin-template": "^14.1.2",
        "@angular-eslint/schematics": "^14.1.2",
        "@angular-eslint/template-parser": "^14.1.2",
        "@angular/cli": "^14.2.3",
        "@angular/compiler-cli": "^14.2.3",
        "@angular/language-service": "^14.2.3",
        "@bartholomej/ngx-translate-extract": "^8.0.2",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@types/file-saver": "^2.0.5",
        "@types/hammerjs": "^2.0.41",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/jquery": "^3.5.14",
        "@types/jqueryui": "^1.12.16",
        "@types/lodash": "^4.14.186",
        "@types/node": "^14.10.2",
        "@types/p5": "^1.4.3",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "eslint": "^8.23.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsdoc": "39.3.6",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "gzipper": "^4.3.0",
        "jasmine-core": "~3.8.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "^6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "karma-mocha-reporter": "^2.2.5",
        "karma-spec-reporter": "0.0.32",
        "protractor": "~7.0.0",
        "sonar-scanner": "^3.1.0",
        "source-map-explorer": "^2.5.0",
        "standard-version": "^9.0.0",
        "ts-node": "^8.10.2",
        "typescript": "^4.8.3"
    },
    "Concenet_FrontVersion": "1.1.5"
}
