{
  "name": "Concenet_Front",
  "version": "1.0.0",
  "jenga_FrontVersion": "1.3.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build",
    "postbuild": "gzipper c --verbose ./dist",
    "build_dev": "npm run build -- --configuration=dev",
    "build_qa": "npm run build -- --configuration=qa",
    "build_pre": "npm run build -- --configuration=pre",
    "build_stg": "npm run build -- --configuration=stg",
    "build_production": "npm run build -- --configuration=production",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar": "npm run test:ci && sonar-scanner",
    "extract:translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/es.json ./src/assets/i18n/en.json --sort --format namespaced-json",
    "release:first": "standard-version --no-verify --first-release",
    "release:patch": "standard-version --no-verify --release-as patch",
    "release:minor": "standard-version --no-verify --release-as minor",
    "release:major": "standard-version --no-verify --release-as major",
    "prebundle:report": "npm run build_production",
    "bundle:report": "source-map-explorer dist/Concenet_Front/**/*.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test:ci",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && npm run lint"
    }
  },
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.1",
    "@angular/cdk": "^13.0.1",
    "@angular/common": "~13.0.1",
    "@angular/compiler": "~13.0.1",
    "@angular/core": "~13.0.1",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.0.1",
    "@angular/material": "~13.0.1",
    "@angular/platform-browser": "~13.0.1",
    "@angular/platform-browser-dynamic": "~13.0.1",
    "@angular/router": "~13.0.1",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "messageformat": "^2.3.0",
    "miragejs": "^0.1.40",
    "ngx-logger": "^4.1.9",
    "ngx-translate-messageformat-compiler": "^4.8.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.2",
    "@angular-eslint/builder": "^13.2.1",
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/schematics": "^13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular/cli": "~13.0.2",
    "@angular/compiler-cli": "~13.0.1",
    "@angular/language-service": "~13.0.1",
    "@bartholomej/ngx-translate-extract": "^8.0.2",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@openapitools/openapi-generator-cli": "^2.1.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.10.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "gzipper": "^4.3.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-spec-reporter": "0.0.32",
    "protractor": "~7.0.0",
    "sonar-scanner": "^3.1.0",
    "source-map-explorer": "^2.5.0",
    "standard-version": "^9.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.4"
  }
}
